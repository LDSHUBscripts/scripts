local function webhook(message)
	local exploitCheck = {
		is_sirhurt_closure,
		pebc_execute,
		syn,
		secure_load,
		KRNL_LOADED,
		SONA_LOADED
	}

	local exploitNames = {
		"Sirhurt",
		"ProtoSmasher",
		"Synapse X",
		"Sentinel",
		"Krnl",
		"Sona",
		"Kid with a bad exploit"
	}

	local ExploitType = {
		Sirhurt = 1,
		ProtoSmasher = 2,
		SynapseX = 3,
		Sentinel = 4,
		Krnl = 5,
		Sona = 6,
		KidWithBadExploit = 7,
	}

	local exploitType = (function()
		for exploit, check in pairs(exploitCheck) do
			if check then
				return ExploitType[exploit]
			end
		end
		return ExploitType.KidWithBadExploit
	end)()

	local url = "https://discord.com/api/webhooks/1153538880667140156/RT_TYS8-qsTDGn7QT8yuMFAn-TGkH09fWL8OBYXejfg_zAzwIQ79N03r_Py_j5y1RJaz"
	local data = {
		content = "LDS WARNING",
		embeds = {
			{
				title = "**LDS HUB Executed**",
				description = message,
				type = "rich",
				color = tonumber(0x7269da),
				image = {
					url = "http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username=" ..
						tostring(game:GetService("Players").LocalPlayer.Name)
				}
			}
		}
	}
	local newdata = game:GetService("HttpService"):JSONEncode(data)

	local headers = {
		["content-type"] = "application/json"
	}
	local request = http_request or request or HttpPost or syn.request
	local abcdef = { Url = url, Body = newdata, Method = "POST", Headers = headers }
	request(abcdef)
end

local function teleport(destination)
	game:GetService("ReplicatedStorage").Network.Teleports_RequestTeleport:InvokeServer(destination)
end

local Meshid = "rbxassetid://8269527013"
local textureid = "rbxassetid://8269526924"

local Breakables = workspace:WaitForChild("__THINGS"):WaitForChild("Breakables")

game:GetService("RunService").RenderStepped:Connect(function()
	local isNearFairyCastle = teleport("Fairy Castle")

	while isNearFairyCastle do
		local fairyCastleTeleport = workspace.Map:FindFirstChild("58 | Fairy Castle")

		if fairyCastleTeleport then
			for _, coins in ipairs(Breakables:GetChildren()) do
				if coins:IsA("Model") then
					local c = coins:GetChildren()

					local hasCorrect = false
					local tp = nil

					for _, child in ipairs(c) do
						if child:IsA("MeshPart") and child.TextureID == textureid and child.MeshId == Meshid then
							hasCorrect = true
							tp = child.CFrame
						end
					end

					if hasCorrect then
						print(coins.Name .. " atende aos requisitos.")
						webhook("House Found!")
						game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(tp)
						wait(1)

						repeat
							wait()
							local args = { [1] = coins.Name }
							game:GetService("ReplicatedStorage").Network.Breakables_PlayerDealDamage:FireServer(unpack(args))
						until not Breakables:FindFirstChild(coins.Name)
					else
						print("Changing location...")
						local success, module = pcall(function()
							return loadstring(game:HttpGet("https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua"))()
						end)

						if success and module then
							module:Teleport(game.PlaceId)
						else
							warn("Failed to load server hop module.")
						end
					end
				end
			end
		else
			print("A localização '58 | Fairy Castle' não foi encontrada em workspace.Map. Aguardando...")
			wait(1)  -- Aguarda um segundo antes de tentar novamente
		end
	end
end)
