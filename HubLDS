	ScreenGui.NameMaterial.TextColor3 = Color3.fromRGB(85, 255, 0)

	local args = {
		[1] = {
			["BoosterPaint"] = {
				["Paint2"] = {
					["Material"] = ScreenGui.NameMaterial.Name
				},
				["Paint"] = {
					["Color"] = {
						[1] = 0.6392157077789307,
						[2] = 0.6352941393852234,
						[3] = 0.6470588445663452
					}
				},
				["Paint3"] = {
					["Color"] = {
						[1] = 0,
						[2] = 1,
						[3] = 0.6352941393852234
					},
					["Material"] = ScreenGui.NameMaterial.Name
				}
			},
			["RPM"] = true,
			["Scraps"] = 1634,
			["BodyPaint"] = {
				["Paint2"] = {
					["Color"] = {
						[1] = 0,
						[2] = 1,
						[3] = 0.6352941393852234
					},
					["Material"] = ScreenGui.NameMaterial.Name
				},
				["Paint"] = {
					["Material"] = ScreenGui.NameMaterial.Name
				}
			},
			["Combustion"] = true,
			["WindowTint"] = 0.75,
			["Airflow"] = true
		},
		[2] = ScreenGui.NamecarAutoFarm.Text
	}

	game:GetService("ReplicatedStorage"):WaitForChild("rE"):WaitForChild("SaveCustoms"):FireServer(unpack(args))

end)

--Modifiers >

--Speed Car
local handle = ScreenGui.ButtonslideSC
local slider = ScreenGui.SliderSC
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local dragging = false

handle.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		while dragging  do
			local percentage = math.round(Snap(math.clamp((mouse.X - slider.AbsolutePosition.X)/slider.AbsoluteSize.X, 0, 1), .02)*100)/100
			handle.Position = UDim2.new(percentage, 0, handle.Position.Y.Scale, 0)
			task.wait(.01)
			local value = ((-0 * (1 - percentage)) + (100 * percentage))
			print(value)
			handle.Text = value
		end
	end
end)

handle.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = false
	end
end)

function Snap(value, snapValue)
	return value - (value % snapValue)
end

ScreenGui.ButtonSpeedCar.MouseButton1Click:Connect(function()
	ScreenGui.Warn.Visible = true
	ScreenGui.Wwarn.Visible = true
end)

