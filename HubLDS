--ToogleGui

local FrameObject = ScreenGui.Frame
local Open = false

local PositionClosed = UDim2.new(-0.317, 0,0351, 0)
local PositionOpen = UDim2.new(0, 407,0, 255)

local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(KeyCode)
	if KeyCode.KeyCode == Enum.KeyCode.RightShift then
		if Open then
			FrameObject:TweenPosition((PositionClosed), "Out", "Quart")
			Open = false
		else
			Open = true
			FrameObject:TweenPosition((PositionOpen), "Out", "Quint")
		end
	end
end)

--Active

ScreenGui.ActiveToogleTxt.MouseButton1Click:Connect(function()
	ScreenGui.Warn.Visible = true
	ScreenGui.Wwarn.Visible = true
end)

--Abrir UI
local function toggleFrame(frame, button)
	local isVisible = not frame.Visible
	frame.Visible = isVisible
	button.BackgroundColor3 = isVisible and Color3.fromRGB(170, 0, 255) or Color3.fromRGB(126, 126, 126)
end

local function hideFrames()
	ScreenGui.HomeUi.Visible = false
	ScreenGui.AutoFarmUi.Visible = false
	ScreenGui.CoreEscapeUI.Visible = false
	ScreenGui.SelectMaterialUI.Visible = false
	ScreenGui.Modifiers_2.Visible = false
	ScreenGui.SettingsUI.Visible = false
end

local function resetButtons()
	ScreenGui.Home.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	ScreenGui.AutoFarm.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	ScreenGui.CoreEscape.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	ScreenGui.SelectMaterial.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	ScreenGui.Modifiers.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	ScreenGui.Settings.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
end

local function resetFrames(excludeFrame, excludeButton)
	hideFrames()
	resetButtons()

	if excludeFrame and excludeButton then
		excludeFrame.Visible = true
		excludeButton.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
	end
end

ScreenGui.Home.MouseButton1Click:Connect(function()
	toggleFrame(ScreenGui.HomeUi, ScreenGui.Home)
	resetFrames(ScreenGui.HomeUi, ScreenGui.Home)
end)

ScreenGui.AutoFarm.MouseButton1Click:Connect(function()
	toggleFrame(ScreenGui.AutoFarmUi, ScreenGui.AutoFarm)
	resetFrames(ScreenGui.AutoFarmUi, ScreenGui.AutoFarm)
end)

ScreenGui.CoreEscape.MouseButton1Click:Connect(function()
	toggleFrame(ScreenGui.CoreEscapeUI, ScreenGui.CoreEscape)
	resetFrames(ScreenGui.CoreEscapeUI, ScreenGui.CoreEscape)
end)

ScreenGui.SelectMaterial.MouseButton1Click:Connect(function()
	toggleFrame(ScreenGui.SelectMaterialUI, ScreenGui.SelectMaterial)
	resetFrames(ScreenGui.SelectMaterialUI, ScreenGui.SelectMaterial)
end)

ScreenGui.Modifiers.MouseButton1Click:Connect(function()
	toggleFrame(ScreenGui.Modifiers_2, ScreenGui.Modifiers)
	resetFrames(ScreenGui.Modifiers_2, ScreenGui.Modifiers)
end)

ScreenGui.Settings.MouseButton1Click:Connect(function()
	toggleFrame(ScreenGui.SettingsUI, ScreenGui.Settings)
	resetFrames(ScreenGui.SettingsUI, ScreenGui.Settings)
end)

-- Esconder todos os frames ao iniciar
hideFrames()
ScreenGui.HomeUi.Visible = true

--Auto Farm

local scroll = ScreenGui.ScrollingFrameAutoFarm
local textBox = ScreenGui.TxtBoxAutoFarm

textBox.Changed:Connect(function()
	local text = textBox.Text:lower()
	if text ~= "" then
		local buttons = scroll:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				local buttonText = button.Text:lower()
				if string.find(buttonText, text) then 
					button.Visible = true 
				else 
					button.Visible = false
				end
			end
		end
	else 
		local buttons = scroll:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				button.Visible = true
			end
		end
	end
end)

local folder = game:GetService("ReplicatedStorage").VehicleInformation

for i, v in pairs (folder:GetChildren()) do

	local TxtButtonAutoFarm = Instance.new("TextButton")
	local CarTesteName = Instance.new("TextLabel")
	local UICorner_28 = Instance.new("UICorner")

	TxtButtonAutoFarm.Name =  v:FindFirstChild("Name").Value
	TxtButtonAutoFarm.Parent = ScreenGui.ScrollingFrameAutoFarm
	TxtButtonAutoFarm.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TxtButtonAutoFarm.Size = UDim2.new(0.900701046, 0, 0.00079999998, 0)
	TxtButtonAutoFarm.Font = Enum.Font.SourceSans
	TxtButtonAutoFarm.Text =  v:FindFirstChild("Name").Value
	TxtButtonAutoFarm.TextColor3 = Color3.fromRGB(0, 0, 0)
	TxtButtonAutoFarm.TextScaled = true
	TxtButtonAutoFarm.TextSize = 14.000
	TxtButtonAutoFarm.TextWrapped = true
	UICorner_28.Parent = TxtButtonAutoFarm

	CarTesteName.Name = v.Name
	CarTesteName.Parent = TxtButtonAutoFarm
	CarTesteName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CarTesteName.BackgroundTransparency = 1.000
	CarTesteName.Font = Enum.Font.SourceSans
	CarTesteName.TextColor3 = Color3.fromRGB(0, 0, 0)
	CarTesteName.TextSize = 14.000

	TxtButtonAutoFarm.MouseButton1Click:Connect(function()

		ScreenGui.NamecarAutoFarm.Text = CarTesteName.Name

	end)

end

local G = getgenv()

G.AutoFarm = false

ScreenGui.autofarm2.MouseButton1Click:Connect(function()
	local plr = game:GetService("Players").LocalPlayer
	local CarName = ScreenGui.NamecarAutoFarm.Text

	G.AutoFarm = not G.AutoFarm
	wait(0.1)

	if G.AutoFarm then
		ScreenGui.Frame_8.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		ScreenGui.Frame_8.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.AutoFarm do
		task.wait()
		if game:GetService("Workspace").CarCollection:FindFirstChild(plr.Name) then
			local Car = game:GetService("Workspace").CarCollection:FindFirstChild(plr.Name)
			pcall(function()
				local mainHitbox = Car.Car.Body.HitBoxes.Main
				mainHitbox.CFrame = mainHitbox.CFrame * CFrame.new(0, 60, 0)
				mainHitbox.Velocity = mainHitbox.CFrame.lookVector * 1000
				task.wait(0.5)
			end)
			if plr.SpawnTimer.Value == 0 then
				game:GetService("ReplicatedStorage").rF.SpawnVehicle:InvokeServer(CarName)
				task.wait(2)
			end
		else
			game:GetService("ReplicatedStorage").rF.SpawnVehicle:InvokeServer(CarName)
		end
	end
end)


G.AutoFarm1 = false

ScreenGui.autofarm1.MouseButton1Click:Connect(function()
	G.AutoFarm1 = not G.AutoFarm1

	if G.AutoFarm1 then
		ScreenGui.Frame_7.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		ScreenGui.Frame_7.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	
	-- services
	local players = game:GetService("Players")
	local vu = game:GetService("VirtualUser")

	-- variables
	local lp = players.LocalPlayer
	local carCollection = workspace.CarCollection
	local guiScript = getsenv(lp.PlayerGui:WaitForChild("GUIs"))
	local openFunc = guiScript["OpenDealership"]
	local spawnFunc = guiScript["SpawnButton"]
	local doBreak = false

	-- functions
	local function getCurrentCar()
		local car = carCollection:FindFirstChild(lp.Name)
		if not car then return nil end

		local model = car:FindFirstChild("Car")
		if not model then return nil end

		local isNotBroken =
			model:FindFirstChild("Wheels"):FindFirstChildOfClass("Part") and
			model:FindFirstChild("Body"):FindFirstChild("Engine"):FindFirstChildOfClass("MeshPart")

		return isNotBroken and model or nil
	end

	local function getCharacter()
		return lp.Character or lp.CharacterAdded:Wait()
	end

	local function canSpawn()
		return lp.SpawnTimer.Value <= 0
	end

	local function spawnBestCar()
		openFunc()
		spawnFunc(true, Enum.UserInputState.Begin)
	end

	local function destroyCar()
		local hum = getCharacter():FindFirstChildOfClass("Humanoid")
		local hrp = getCharacter():FindFirstChild("HumanoidRootPart")

		if not hum or not hrp then return end

		local car = getCurrentCar()

		repeat task.wait() until car.PrimaryPart ~= nil

		-- Destruction of the car!!!
		repeat task.wait()
			car = getCurrentCar()
			if not car then return end

			task.wait(1)

			pcall(function()
				if not car.PrimaryPart then return end
				car.PrimaryPart.CFrame = car.PrimaryPart.CFrame * CFrame.new(0,70,0)
				car.PrimaryPart.Velocity = car.PrimaryPart.CFrame.LookVector * 1000
			end)

			task.wait(1)

			pcall(function()
				if not car.PrimaryPart then return end
				car.PrimaryPart.CFrame = car.PrimaryPart.CFrame * CFrame.new(0,70,0)
				car.PrimaryPart.Velocity = car.PrimaryPart.CFrame.LookVector * 1000
			end)

		until not doBreak
	end

	-- main
	lp.Idled:Connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)

	while task.wait() do
		if not G.AutoFarm1 then
			break
		end

		local character = getCharacter()

		if not character then return end

		if canSpawn() then
			doBreak = true
			task.delay(10, function() doBreak = false end)
			pcall(function()
				spawnBestCar()
				destroyCar()
			end)
		end
	end
end)


-- Select Material

local scroll = ScreenGui.SFCarMaterial
local textBox = ScreenGui.TxtBoxMaterial

textBox.Changed:Connect(function()
	local text = textBox.Text:lower()
	if text ~= "" then
		local buttons = scroll:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				local buttonText = button.Text:lower()
				if string.find(buttonText, text) then 
					button.Visible = true 
				else 
					button.Visible = false
				end
			end
		end
	else 
		local buttons = scroll:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				button.Visible = true
			end
		end
	end
end)

local Pintura = game:GetService("Players").LocalPlayer.PlayerGui.Dealership.Customize.Paint.Materials.Grid

for i, v in pairs(Pintura:GetChildren()) do

	if v:IsA("Frame") then
		local TxtButtonMaterial = Instance.new("TextButton")
		local UICorner_41 = Instance.new("UICorner")

		TxtButtonMaterial.Name = v.Name
		TxtButtonMaterial.Parent = ScreenGui.SFCarMaterial
		TxtButtonMaterial.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TxtButtonMaterial.Size = UDim2.new(0.900701046, 0, 0.119999997, 0)
		TxtButtonMaterial.Font = Enum.Font.SciFi
		TxtButtonMaterial.Text = v.Name
		TxtButtonMaterial.TextColor3 = Color3.fromRGB(0, 0, 0)
		TxtButtonMaterial.TextScaled = true
		TxtButtonMaterial.TextSize = 14.000
		TxtButtonMaterial.TextWrapped = true
		UICorner_41.Parent = TxtButtonMaterial

		ScreenGui.TxtButtonMaterial.MouseButton1Click:Connect(function()

			ScreenGui.NameMaterial.TextColor3 = Color3.fromRGB(255, 0, 0)
			ScreenGui.NameMaterial.Name = ScreenGui.TxtButtonMaterial.Text
		end)
	end
end

--Colocar Material
