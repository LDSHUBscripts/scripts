						if isInClamEgg then
							repeat
								local args = {
									[1] = "Clam",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet()
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInKingFishEgg then
							repeat
								local args = {
									[1] = "KingFish",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInPoseidonEgg then
							repeat
								local args = {
									[1] = "Poseidon",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end 
					end
				end
			end
		end
	end
	wait(1)
	checkPetIndex(index + 1)
end

local function AutoIndex()

	if G.Settings.AutoIndex == true then
		LDSHUB.AutoFarmFrameColor2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoFarmFrameColor2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if G.Settings.AutoIndex == true then	
		checkPetIndex(1)
	end
end


--Bosses 

local Bosses1 = {
	"Bully",
	"Teacher",
	"GymRat",
	"MafiaBoss",
	"Champion"
}
local Bosses2 = {
	"ScrapTrader",
	"CyberCop",
	"SlicerAssassin",
	"RipperDoc",
	"RogueAi"
}
local Bosses3 = {
	"CocoNut",
	"FatPirate",
	"MermaidKing",
	"FriendlyShark",
	"KrakenBoss"
}

local bossW1 = false
local bossW2 = false
local bossW3 = false

local function CheckIfTextInTable(text, tbl)
	for _, value in ipairs(tbl) do
		if value == text then
			return true
		end
	end
	return false
end

local function CreateTextButtonBoss(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEventBoss(button, namePetObject, bossVariable)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
		bossVariable = true
	end)
end

local function resetboss()
	bossW1 = false
	bossW2 = false
	bossW3 = false
end

for _, BossName in ipairs(Bosses1) do
	local button = CreateTextButtonBoss(LDSHUB.ScrollingFrameBoss, BossName)
	ConnectButtonClickEventBoss(button, LDSHUB.NameBoss, bossW1)
end

for _, BossName in ipairs(Bosses2) do
	local button = CreateTextButtonBoss(LDSHUB.ScrollingFrameBoss, BossName)
	ConnectButtonClickEventBoss(button, LDSHUB.NameBoss, bossW2)
end

for _, BossName in ipairs(Bosses3) do
	local button = CreateTextButtonBoss(LDSHUB.ScrollingFrameBoss, BossName)
	ConnectButtonClickEventBoss(button, LDSHUB.NameBoss, bossW3)
end

local function BossFight()
	local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
	if Ui.Visible == false then
		if CheckIfTextInTable(LDSHUB.NameBoss.Text, Bosses1) then
			bossW1 = true
			local args = {
				[1] = LDSHUB.NameBoss.Text,
				[2] = workspace:WaitForChild("Zones"):WaitForChild("1"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(LDSHUB.NameBoss.Text),
				[3] = "1"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
		elseif CheckIfTextInTable(LDSHUB.NameBoss.Text, Bosses2) then
			bossW2 = true
			local args = {
				[1] = LDSHUB.NameBoss.Text,
				[2] = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(LDSHUB.NameBoss.Text),
				[3] = "2"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
		elseif CheckIfTextInTable(LDSHUB.NameBoss.Text, Bosses3) then
			bossW3 = true
			local args = {
				[1] = LDSHUB.NameBoss.Text,
				[2] = workspace:WaitForChild("Zones"):WaitForChild("3"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(LDSHUB.NameBoss.Text),
				[3] = "3"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
		end
	end
end

local function AutoBoss()

	if G.Settings.AutoBoss == true then
		LDSHUB.AutoFarmFrameColor3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoFarmFrameColor3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoBoss do
		BossFight()
		wait(1)
	end
end

