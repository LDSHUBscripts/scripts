end)

-- Esconder todos os frames ao iniciar
hideFrames()
LDSHUB.HomeUi.Visible = true

--Event script

local G = getgenv()

local function AutoEventEgg()
	if G.Settings.AutoEvent1 == true then
		LDSHUB.EventFrame1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent1 do
		local quantidade = LDSHUB.TextBoxEggsEvent.Text
		local args = {
			[1] = quantidade
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EventService"):WaitForChild("RF"):WaitForChild("ClaimEgg"):InvokeServer(unpack(args))
	end
end

local function AutoEvent1()

	if G.Settings.AutoEvent2 == true then
		LDSHUB.EventFrame2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent2 do
		local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
		local Numeros = 1
		local npcData = {
			[1] = {
				name = 	   "ScrapTrader",
				npc = nil,
			},
			[2] = {
				name = 	   "CyberCop",
				npc = nil,
			},
			[3] = {
				name = "SlicerAssassin",
				npc = nil,
			}
		}
		repeat
			if Ui.Visible == false then
				local data = npcData[Numeros]
				local npc = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
				if npc then
					data.npc = npc
					local args = {
						[1] = data.name,
						[2] = npc,
						[3] = "2"
					}	
					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
				end
			end
			-- Passar para o próximo NPC
			Numeros = Numeros + 1
		until Numeros > #npcData
		wait(0.1)
	end
end

local function AutoEvent2()

	if G.Settings.AutoEvent3 == true then
		LDSHUB.EventFrame3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent3 do
		local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
		local Numeros = 1
		local npcData = {
			[1] = {
				name = 	  "CocoNut",
				npc = nil,
			}
		}
		repeat
			if Ui.Visible == false then
				local data = npcData[Numeros]
				local npc = workspace:WaitForChild("Zones"):WaitForChild("3"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
				if npc then
					data.npc = npc
					local args = {
						[1] = data.name,
						[2] = npc,
						[3] = "3"
					}	
					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
				end
			end
			-- Passar para o próximo NPC
			Numeros = Numeros + 1
		until Numeros > #npcData
		wait(0.1)
	end
end

-- AutoFarm scripts

local function AutoClick()

	if G.Settings.AutoClick == true then
		LDSHUB.AutoFarmFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoFarmFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoClick == true do
		wait(0.1)
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onClickRequest"):FireServer()
	end
end

