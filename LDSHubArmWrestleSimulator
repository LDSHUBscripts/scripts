--Drag Ui
Frame.Draggable = true
Frame.Selectable = true
Frame.Active = true

--ToogleGui

local FrameObject = Frame
local Open = false

local PositionClosed = UDim2.new(-0.317, 0,0351, 0)
local PositionOpen = UDim2.new(0, 407,0, 255)

local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(KeyCode)
	if KeyCode.KeyCode == Enum.KeyCode.RightShift then
		if Open then
			FrameObject:TweenPosition((PositionClosed), "Out", "Quart")
			Open = false
		else
			Open = true
			FrameObject:TweenPosition((PositionOpen), "Out", "Quint")
		end
	end
end)

--Active

ActiveToogleTxt.MouseButton1Click:Connect(function()
	Warn.Visible = true
	Wwarn.Visible = true
end)

--Abrir UI
local function toggleFrame(frame, button)
	local isVisible = not frame.Visible
	frame.Visible = isVisible
	button.BackgroundColor3 = isVisible and Color3.fromRGB(170, 0, 255) or Color3.fromRGB(126, 126, 126)
end

local function hideFrames()
	HomeUi.Visible = false
	AutoFarmUi.Visible = false
	SettingsUI.Visible = false
	UpdateNoteUi.Visible = false
end

local function resetButtons()
	Home.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	AutoFarm.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	Settings.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	UpdateNotes.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
end

local function resetFrames(excludeFrame, excludeButton)
	hideFrames()
	resetButtons()

	if excludeFrame and excludeButton then
		excludeFrame.Visible = true
		excludeButton.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
	end
end

Home.MouseButton1Click:Connect(function()
	toggleFrame(HomeUi, Home)
	resetFrames(HomeUi, Home)
end)

AutoFarm.MouseButton1Click:Connect(function()
	toggleFrame(AutoFarmUi, AutoFarm)
	resetFrames(AutoFarmUi, AutoFarm)
end)

Settings.MouseButton1Click:Connect(function()
	toggleFrame(SettingsUI, Settings)
	resetFrames(SettingsUI, Settings)
end)

UpdateNotes.MouseButton1Click:Connect(function()
	toggleFrame(UpdateNoteUi, UpdateNotes)
	resetFrames(UpdateNoteUi, UpdateNotes)
end)

-- Esconder todos os frames ao iniciar
hideFrames()
HomeUi.Visible = true

-- Auto Farm
local G = getgenv()

G.AutoClick = false
autofarm1.MouseButton1Click:Connect(function()
	
	G.AutoClick = not G.AutoClick
	
	if G.AutoClick then
		Frame_6.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		Frame_6.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	
	while G.AutoClick do
		wait(0.1)
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onClickRequest"):FireServer()
	end
end)

G.AutoFarm = false

autofarm2.MouseButton1Click:Connect(function()
	
	G.AutoFarm = not G.AutoFarm
	
	if G.AutoFarm then
		Frame_7.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		Frame_7.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	
	while G.AutoFarm do
	local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
	local Numeros = 1

	local G = getgenv()

	G.AutoClick = false

	local npcData = {
		[1] = {
			name = "Bully",
			npc = nil,
		},
		[2] = {
			name = "Teacher",
			npc = nil,
		},
		[3] = {
			name = "GymRat",
			npc = nil,
		},
		[4] = {
			name = "MafiaBoss",
			npc = nil,
		},
		[5] = {
			name = "Champion",
			npc = nil,
		}
	}

	repeat
		if Ui.Visible == false then
			local data = npcData[Numeros]
			print("Verificando NPC:", data.name)
			local npc = workspace:WaitForChild("Zones"):WaitForChild("1"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
			if npc then
				print("NPC encontrado:", data.name)
				data.npc = npc
				local args = {
					[1] = data.name,
					[2] = npc,
					[3] = "1"
				}	

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
				print("NPC selecionado:", data.name)

				-- Aguardar até que o NPC seja derrotado
				wait(2)
				repeat
					wait()
				until Ui.Visible == false
				print("NPC derrotado:", data.name)
			else
				print("NPC não encontrado:", data.name)
			end
		end

		-- Passar para o próximo NPC
		Numeros = Numeros + 1
		until Numeros > #npcData
		
		-- teleport
		
		local args = {
			[1] = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("Futuristic")
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		
		-- segundo mundo
		
		wait(1)
	
		local Numeros1 = 1

		local npcData1 = {
			[1] = {
				name1 = "ScrapTrader",
				npc1 = nil,
			},
			[2] = {
				name1 = "CyberCop",
				npc1 = nil,
			},
			[3] = {
				name1 = "SlicerAssassin",
				npc1 = nil,
			},
			[4] = {
				name1 = "RipperDoc",
				npc1 = nil,
			},
			[5] = {
				name1 = "RogueAi",
				npc1 = nil,
			}
		}

		repeat
			if Ui.Visible == false then
				local data = npcData1[Numeros1]
				print("Verificando NPC:", data.name1)
				local npc = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name1)
				if npc then
					print("NPC encontrado:", data.name1)
					data.npc1 = npc
					local args = {
						[1] = data.name1,
						[2] = npc,
						[3] = "2"
					}

					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
					print("NPC selecionado:", data.name1)

					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
					print("NPC derrotado:", data.name1)
				else
					print("NPC não encontrado:", data.name1)
				end
			end

			-- Passar para o próximo NPC
			Numeros1 = Numeros1 + 1
		until Numeros1 > #npcData1

		
		--teleport
		
		local args = {
			[1] = workspace:WaitForChild("Zones"):WaitForChild("1"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("BackToSchool")
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))

		wait(1)
	end
end)
