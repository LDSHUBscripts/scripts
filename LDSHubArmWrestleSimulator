hideFrames()
LDSHUB.HomeUi.Visible = true

--Event script

local G = getgenv()

--Auto Egg Event

G.AutoEggEvent = false

LDSHUB.EventButton1.MouseButton1Click:Connect(function()

	G.AutoEggEvent = not G.AutoEggEvent

	if G.AutoEggEvent == true then
		LDSHUB.Frame_8.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.Frame_8.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.AutoEggEvent do

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EventService"):WaitForChild("RF"):WaitForChild("ClaimEgg"):InvokeServer()
		wait(.5)
	end

end)

--Auto Farm World 1 

G.AutoFarmEvent1 = false

LDSHUB.EventButton2.MouseButton1Click:Connect(function()

	G.AutoFarmEvent1 = not G.AutoFarmEvent1

	if G.AutoFarmEvent1 == true then
		LDSHUB.Frame_9.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.Frame_9.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.AutoFarmEvent1 do
		local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
		local Numeros = 1

		local npcData = {
			[1] = {
				name = "Bully",
				npc = nil,
			},
			[2] = {
				name = "Teacher",
				npc = nil,
			},
			[3] = {
				name = "GymRat",
				npc = nil,
			},
			[4] = {
				name = "MafiaBoss",
				npc = nil,
			},
			[5] = {
				name = "Champion",
				npc = nil,
			}
		}

		repeat
			if Ui.Visible == false then
				local data = npcData[Numeros]
				local npc = workspace:WaitForChild("Zones"):WaitForChild("1"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
				if npc then

					data.npc = npc
					local args = {
						[1] = data.name,
						[2] = npc,
						[3] = "1"
					}	

					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))

					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
				end
			end

			-- Passar para o próximo NPC
			Numeros = Numeros + 1
		until Numeros > #npcData
		wait(0.1)
	end
end)

--Auto Farm World 2

G.AutoFarmEvent2 = false

LDSHUB.EventButton3.MouseButton1Click:Connect(function()

	G.AutoFarmEvent2 = not G.AutoFarmEvent2

	if G.AutoFarmEvent2 == true then
		LDSHUB.Frame_10.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.Frame_10.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	local args = {
		[1] = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("Futuristic")
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
	wait(.2)
	while G.AutoFarmEvent2 do
		local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
		local Numeros = 1

		local npcData = {
			[1] = {
				name = "ScrapTrader",
				npc = nil,
			},
			[2] = {
				name = "CyberCop",
				npc = nil,
			}
		}

		repeat
			if Ui.Visible == false then
				local data = npcData[Numeros]
				local npc = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
				if npc then
					data.npc = npc
					local args = {
						[1] = data.name,
						[2] = npc,
						[3] = "2"
					}

					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))

					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
				end
			end

			-- Passar para o próximo NPC
			Numeros = Numeros + 1
		until Numeros > #npcData
		wait(.1)
	end
end)

