local EggsName = {
	"Earth",
	"Icy",
	"Lava",
	"Blackhole",
	"Crystal",
	"Molten",
	"Solar",
	"Moon",
	"Ice",
	"Burning",
	"Coconut",
	"Palm",
	"Treasure",
	"Clam",
	"KingFish",
	"Poseidon"
}

local function CreateTextButtonEgg(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEventegg(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
	end)
end

for _, eggName in pairs(EggsName) do
	local button = CreateTextButtonEgg(LDSHUB.ScrollingFrameEgg, eggName)
	ConnectButtonClickEventegg(button, LDSHUB.NameEgg) -- Modifiquei aqui para passar diretamente LDSHUB.NameEgg
end

local function AutoEgg()

	if G.Settings.AutoEgg == true then
		LDSHUB.AutoEggFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoEggFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoEgg do
		local args = {
			[1] = LDSHUB.NameEgg.Text,
			[2] = {},
			[4] = false
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
		wait(1.5)
	end
end

local function VisibleEgg()
	local plr = game.Players.LocalPlayer
	local gui = plr.PlayerGui.OpenerUI


	if G.Settings.RemoveEggAnimation == true then
		LDSHUB.VisibleEggFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.VisibleEggFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if G.Settings.RemoveEggAnimation == true then
		gui.Enabled = false
	else
		gui.Enabled = true
	end
end

