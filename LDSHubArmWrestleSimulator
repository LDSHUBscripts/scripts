-- Misc Scripts

local function AutoClaimGift()
	G.Settings.AutoClaimReward = not G.Settings.AutoClaimReward

	if G.Settings.AutoClaimReward == true then
		LDSHUB.AutoClaimFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoClaimFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoClaimReward do
		for i = 1, 12 do
			local args = {
				[1] = tostring(i)
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("TimedRewardService"):WaitForChild("RE"):WaitForChild("onClaim"):FireServer(unpack(args))
			wait(0.5)
		end
		wait(10)
	end
end

local function AutoSping()
	local plr = game.Players.LocalPlayer
	local gui = plr.PlayerGui.GameUI.Menus.Spin.Spin

	if gui.Visible == true then
		G.Settings.AutoSpinReward = true
	else
		G.Settings.AutoSpinReward = false
	end

	if G.Settings.AutoSpinReward == true then
		LDSHUB.AutoClaimFrameColor2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoClaimFrameColor2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoSpinReward do
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("SpinService"):WaitForChild("RE"):WaitForChild("onSpinRequest"):FireServer()
		wait(0.1)
		if gui.Visible == false then
			G.Settings.AutoSpinReward = false
		end
	end
end

--Auto Delete

local PetsEvent = {
	"NinjaPanda",
	"NinjaParrot",
	"NinjaYeti",
	"NinjaSquirrel"
}

local function CreateTextButton(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEvent(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
	end)
end

for _, petName in ipairs(PetsEvent) do
	local button1 = CreateTextButton(LDSHUB.ScrollingFramePet1, petName)
	ConnectButtonClickEvent(button1, LDSHUB.NamePet)

	local button2 = CreateTextButton(LDSHUB.ScrollingFramePet2, petName)
	ConnectButtonClickEvent(button2, LDSHUB.NamePet_2)

	local button3 = CreateTextButton(LDSHUB.ScrollingFramePet3, petName)
	ConnectButtonClickEvent(button3, LDSHUB.NamePet_3)
end

local function resetsc()
	LDSHUB.PetName1.Visible = false
	LDSHUB.PetName2.Visible = false
	LDSHUB.PetName3.Visible = false
end

LDSHUB.ButtonDelete1.MouseButton1Click:Connect(function()
	resetsc()
	LDSHUB.PetName1.Visible = true
end)

LDSHUB.ButtonDelete2.MouseButton1Click:Connect(function()
	resetsc()
	LDSHUB.PetName2.Visible = true
end)

LDSHUB.ButtonDelete3.MouseButton1Click:Connect(function()
	resetsc()
	LDSHUB.PetName3.Visible = true
end)

local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets

local function AutoDelete()
	local namePetR1 = LDSHUB.NamePet.Text
	local namePetR2 = LDSHUB.NamePet_2.Text
	local namePetR3 = LDSHUB.NamePet_3.Text
	while G.AutoDelete do
		for i, v in pairs(PetInv:GetChildren()) do
			if v:IsA("Frame") then
				wait()
				local toggle = v.Toggle
				if toggle then
					local attributeName = "Key"
					local attributeId = "GUID"

					local attributeValue = toggle:GetAttribute(attributeName)
					local attributeValueid = toggle:GetAttribute(attributeId)

					if attributeValue then
						if attributeValue == namePetR1 or attributeValue == namePetR2 or attributeValue == namePetR3 then
							if attributeValueid then
								local args = {
									[1] = attributeValueid
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PetService"):WaitForChild("RF"):WaitForChild("delete"):InvokeServer(unpack(args))
							end
						end
					end
				end
			end
		end
	end
end

local function AutoDeleteEvent()

	if G.Settings.AutoDelete == true then
		LDSHUB.DeleteButtonFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.DeleteButtonFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	for i = 1, 10 do
		spawn(function()
			AutoDelete()
		end)
	end
end

-- Auto Converts

local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets

local function AutoCraft()
	for i, v in pairs(PetInv:GetChildren()) do
		if v:IsA("Frame") then
			wait()
			local toggle = v.Toggle
			if toggle then
				local attributeId = "GUID"
				local attributeCraft = "CraftType"

				local attributeValueid = toggle:GetAttribute(attributeId)
				local attributeCraft = toggle:GetAttribute(attributeCraft)

				if attributeValueid then
					if attributeCraft == "Big" or attributeCraft == "Baby" then
						print(attributeValueid)
						wait(2.5)
						local args = {
							[1] = attributeValueid,
							[2] = true
						}

						game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PetService"):WaitForChild("RF"):WaitForChild("craft"):InvokeServer(unpack(args))
					end
					print("huge pet")
				end
			else
				print("pet nao encontrado")
			end
		else
			print("pet nao encontrado")
		end
	end
end

local function AutoCraftPets()

	if G.Settings.AutoCraft == true then
		LDSHUB.ConvertFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.ConvertFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if G.Settings.AutoCraft == true then
		AutoCraft()
	end
end

LDSHUB.autoconvert2.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = true
	LDSHUB.Wwarn.Visible = true
end)

