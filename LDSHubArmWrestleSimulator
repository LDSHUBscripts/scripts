-- Visual Pets

local function VisualSerpents()
	local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets
	local PetStats = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.RightBar
	for i, v in pairs(PetInv:GetChildren()) do
		if v:IsA("Frame") then
			wait() -- Pode ser necessário ajustar a duração deste wait() para evitar atrasos excessivos
			local toggle = v:FindFirstChild("Toggle") -- Corrigido para usar FindFirstChild em vez de acessar diretamente a propriedade
			if toggle then
				local PetIcon = toggle:FindFirstChild("PetIcon")
				local Huge = toggle:FindFirstChild("Huge")
				local Big = toggle:FindFirstChild("Big")
				local Stat = toggle:FindFirstChild("Stat")

				if PetIcon and Huge and Big and Stat then -- Verifica se todos os elementos foram encontrados
					local PetIconId = "http://www.roblox.com/asset/?id=13778188391"
					local StatName = "Ultra Serpent"

					local Size = UDim2.new(2.1500001, 0, 2.1500001, 0) -- Corrigido para usar UDim2.new em vez de uma string

					PetIcon.Icon.Image = PetIconId -- Corrigido para definir a propriedade Image em vez de atribuir diretamente ao objeto
					PetIcon.Size = Size
					Huge.Visible = true
					Big.Visible = false -- Corrigido para alterar a propriedade correta para false
					Stat.Text = StatName
					Stat.TextColor3 = Color3.new(0.666667, 0.333333, 1)	
				end
			end
		end
	end
	while true do
		wait()
		local ImportantStats = PetStats:FindFirstChild("Important")
		local PetIconStats = PetStats:FindFirstChild("Icon")
		local NamePetStats = PetStats:FindFirstChild("petName")
		PetIconStats.Image = "rbxassetid://13778188391"
		NamePetStats.Text = "Ultra Serpent"
		NamePetStats.TextColor3 = Color3.new(0.666667, 0.333333, 1)
		ImportantStats.Boost.Text = "20000x Boost"
		ImportantStats.Rarity.Text = "Omega"
		ImportantStats.Exists.Visible = true
		ImportantStats.Exists.Text = "￢ﾜﾨ 1 Exist ￢ﾜﾨ"

	end
end

local function VisualTroll()
	local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets
	local PetStats = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.RightBar
	for i, v in pairs(PetInv:GetChildren()) do
		if v:IsA("Frame") then
			wait() -- Pode ser necessário ajustar a duração deste wait() para evitar atrasos excessivos
			local toggle = v:FindFirstChild("Toggle") -- Corrigido para usar FindFirstChild em vez de acessar diretamente a propriedade
			if toggle then
				local PetIcon = toggle:FindFirstChild("PetIcon")
				local Huge = toggle:FindFirstChild("Huge")
				local Big = toggle:FindFirstChild("Big")
				local Stat = toggle:FindFirstChild("Stat")

				if PetIcon and Huge and Big and Stat then -- Verifica se todos os elementos foram encontrados
					local PetIconId = "http://www.roblox.com/asset/?id=13036234672"
					local StatName = "Sr LDS"

					local Size = UDim2.new(2.1500001, 0, 2.1500001, 0) -- Corrigido para usar UDim2.new em vez de uma string

					PetIcon.Icon.Image = PetIconId -- Corrigido para definir a propriedade Image em vez de atribuir diretamente ao objeto
					PetIcon.Size = Size
					Huge.Visible = true
					Big.Visible = false -- Corrigido para alterar a propriedade correta para false
					Stat.Text = StatName
					Stat.TextColor3 = Color3.new(0.666667, 0.333333, 1)
				end
			end
		end
	end
	while true do
		wait()
		local ImportantStats = PetStats:FindFirstChild("Important")
		local PetIconStats = PetStats:FindFirstChild("Icon")
		local NamePetStats = PetStats:FindFirstChild("petName")
		PetIconStats.Image = "http://www.roblox.com/asset/?id=13036234672"
		NamePetStats.Text = "Sr LDS"
		NamePetStats.TextColor3 = Color3.new(0.666667, 0.333333, 1)
		ImportantStats.Boost.Text = "69000x Boost"
		ImportantStats.Rarity.Text = "Youtuber"
		ImportantStats.Exists.Visible = true
		ImportantStats.Exists.Text = "￢ﾜﾨ 1 Exist ￢ﾜﾨ"

	end
end

local function VisualPotions()

	local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.Store.Content.Potions.Content.Left

	for i, v in pairs(PetInv:GetChildren()) do
		if v:IsA("ImageLabel") then
			v.Buttons.Claim.TextLabel.Text = "USE (99,999)"
		end
	end
end

local function HopGifts()

	if G.Settings.AutoHopSever == true then

		if G.Settings.AutoHopSever == true then
			LDSHUB.AutoClaimFrameColor3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
		else
			LDSHUB.AutoClaimFrameColor3.AutoClaimFrameColor3 = Color3.fromRGB(255, 0, 0)
		end
		wait(5400)
		for i = 1, 12 do
			local args = {
				[1] = tostring(i)
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("TimedRewardService"):WaitForChild("RE"):WaitForChild("onClaim"):FireServer(unpack(args))
			wait(0.5)
		end
		wait(10)

		local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/13127800756/servers/Public?sortOrder=Asc&limit=100"))
		for i,v in pairs(Servers.data) do
			if v.playing ~= v.maxPlayers then
				game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
			end
		end
	end
end

