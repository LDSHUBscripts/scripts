LDSHUB.ModifiersButton1.MouseButton1Click:Connect(function()
	local plr = game.Workspace:FindFirstChild(player.Name)
	if plr then
		local char = plr
		char.Humanoid.WalkSpeed = LDSHUB.ButtonslideSC.Text * 5
	end
end)

LDSHUB.ModifiersButton2.MouseButton1Click:Connect(function()
	local plr = game.Workspace:FindFirstChild(player.Name)
	if plr then
		local char = plr
		char.Humanoid.JumpPower = LDSHUB.ButtonslideSC_2.Text * 5
	end
end)

