-- Verificar o Save
wait(2)
local function executeFunction(func)
	if type(func) == "function" then
		func()
	end
end

local functionsToExecute = {
	["AutoFarm1"] = autofarm1,
	["AutoFarm2"] = autofarm2,
	["Bypass"] = bypass,
	["GetAllItem"] = getallitem,
	["RemoveKillParts"] = removekillparts,

}

for key, func in pairs(functionsToExecute) do
	if G.Settings[key] then
		executeFunction(func)
	end
end
