LDSHUB.Frame.Active = true

--ToogleGui

local FrameObject = LDSHUB.Frame
local Open = false

local PositionClosed = UDim2.new(-0.317, 0,0351, 0)
local PositionOpen = UDim2.new(0, 407,0, 255)

local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(KeyCode)
	if KeyCode.KeyCode == Enum.KeyCode.RightShift then
		if Open then
			FrameObject:TweenPosition((PositionClosed), "Out", "Quart")
			Open = false
		else
			Open = true
			FrameObject:TweenPosition((PositionOpen), "Out", "Quint")
		end
	end
end)

--Abrir UI
local function toggleFrame(frame, button)
	local isVisible = not frame.Visible
	frame.Visible = isVisible
	button.BackgroundColor3 = isVisible and Color3.fromRGB(170, 0, 255) or Color3.fromRGB(126, 126, 126)
end

local function hideFrames()
	LDSHUB.HomeUi.Visible = false
	LDSHUB.AutoFarmUi.Visible = false
	LDSHUB.BypassUi.Visible = false
	LDSHUB.ModifiersUi.Visible = false
	LDSHUB.SettingsUI.Visible = false
	LDSHUB.UpdateNoteUi.Visible = false
	LDSHUB.TeleportsUi.Visible = false
	LDSHUB.MiscUi.Visible = false

end

local function resetButtons()
	LDSHUB.Home.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoFarm.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Bypass.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Modifiers.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Settings.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.UpdateNotes.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Teleports.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Misc.BackgroundColor3 = Color3.fromRGB(126, 126, 126)

end

local function resetFrames(excludeFrame, excludeButton)
	hideFrames()
	resetButtons()

	if excludeFrame and excludeButton then
		excludeFrame.Visible = true
		excludeButton.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
	end
end

LDSHUB.Home.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.HomeUi, LDSHUB.Home)
	resetFrames(LDSHUB.HomeUi, LDSHUB.Home)
end)

LDSHUB.AutoFarm.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.AutoFarmUi, LDSHUB.AutoFarm)
	resetFrames(LDSHUB.AutoFarmUi, LDSHUB.AutoFarm)
end)

LDSHUB.Bypass.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.BypassUi, LDSHUB.Bypass)
	resetFrames(LDSHUB.BypassUi, LDSHUB.Bypass)
end)

LDSHUB.Modifiers.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.ModifiersUi, LDSHUB.Modifiers)
	resetFrames(LDSHUB.ModifiersUi, LDSHUB.Modifiers)
end)

LDSHUB.Settings.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.SettingsUI, LDSHUB.Settings)
	resetFrames(LDSHUB.SettingsUI, LDSHUB.Settings)
end)

LDSHUB.UpdateNotes.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.UpdateNoteUi, LDSHUB.UpdateNotes)
	resetFrames(LDSHUB.UpdateNoteUi, LDSHUB.UpdateNotes)
end)

LDSHUB.Teleports.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.TeleportsUi, LDSHUB.Teleports)
	resetFrames(LDSHUB.TeleportsUi, LDSHUB.Teleports)
end)

LDSHUB.Misc.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.MiscUi, LDSHUB.Misc)
	resetFrames(LDSHUB.MiscUi, LDSHUB.Misc)
end)

-- Esconder todos os frames ao iniciar
hideFrames()
LDSHUB.HomeUi.Visible = true

local G = getgenv()

-- auto farm

local function autofarm1()
	spawn(function()
		while G.Settings.AutoFarm1 do
			local endzone = game.Workspace.tower.sections.finish.FinishGlow.CFrame

			local player = game.Players.LocalPlayer.Character
			player.HumanoidRootPart.CFrame = endzone
			wait()
		end
	end)
end

local function autofarm2()
	spawn(function()
		while G.Settings.AutoFarm2 do
			local endzone = game.Workspace.tower.sections.finish.FinishGlow.CFrame

			local player = game.Players.LocalPlayer.Character
			player.HumanoidRootPart.CFrame = endzone
			wait(2)
			local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/1962086868/servers/Public?sortOrder=Asc&limit=100"))
			for i,v in pairs(Servers.data) do
				if v.playing ~= v.maxPlayers then
					game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
				end
			end

		end
	end)
end

-- bypass

local function bypass()
	local reg = getreg()

	for i, Function in next, reg do
		if type(Function) == 'function' then
			local info = getinfo(Function)

			if info.name == 'kick' then
				if (hookfunction(info.func, function(...)end)) then
					print'succesfully hooked kick'
				else
					print'failed to hook kick'
				end
			end
		end
	end

	local playerscripts = game:GetService'Players'.LocalPlayer.PlayerScripts

	local script1 = playerscripts.LocalScript
	local script2 = playerscripts.LocalScript2

	local script1signal = script1.Changed
	local script2signal = script2.Changed

	for i, connection in next, getconnections(script1signal) do
		connection:Disable()
	end
	for i, connection in next, getconnections(script2signal) do
		connection:Disable()
	end

	script1:Destroy()
	script2:Destroy()
end

local function removekillparts()
	while G.Settings.RemoveKillParts do
		for i,v in pairs(game:GetService("Workspace").tower:GetDescendants()) do
			if v:IsA("BoolValue") and v.Name == "kills" then
				v.Parent:Destroy()
			end
		end
		wait()
	end
end

-- modifiers

local function getallitem()
	for _,e in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
		if e:IsA("Tool") then
			e:Destroy()
		end
	end
	wait() 
	for _,v in pairs(game.ReplicatedStorage.Gear:GetDescendants()) do
		if v:IsA("Tool") then
			local CloneThings = v:Clone()
			wait()
			CloneThings.Parent = game.Players.LocalPlayer.Backpack

		end
	end
end

-- teleports 

local function TPend()
	local endzone = game.Workspace.tower.sections.finish.FinishGlow.CFrame

	local player = game.Players.LocalPlayer.Character
	player.HumanoidRootPart.CFrame = endzone
end

local function tpstart()
	local startzone = game.Workspace.tower.sections.start.center.CFrame

	local player = game.Players.LocalPlayer.Character
	player.HumanoidRootPart.CFrame = startzone
end

local function tpPro()
	local startzone = game.Workspace.tower.sections.start.gate.gate.CFrame

	local player = game.Players.LocalPlayer.Character
	player.HumanoidRootPart.CFrame = startzone
end


-- modificadores

local function UpdateValue(slider, handle, property)
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local dragging = false

	handle.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
		end
	end)

	handle.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local percentage = math.clamp((mouse.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X, 0, 1)
			handle.Position = UDim2.new(percentage, 0, handle.Position.Y.Scale, 0)
			local value = math.floor(percentage * 100)
			handle.Text = value
			player[property] = value -- Atualiza a propriedade do jogador
		end
	end)
end

local player = game.Players.LocalPlayer
local handleSpeed = LDSHUB.ButtonslideSC_2
local sliderSpeed = LDSHUB.SliderSC_2
UpdateValue(sliderSpeed, handleSpeed, "WalkSpeed")

local handleJumpPower = LDSHUB.ButtonslideSC
local sliderJumpPower = LDSHUB.SliderSC
UpdateValue(sliderJumpPower, handleJumpPower, "JumpPower")


